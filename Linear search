#include<stdio.h>
int main(){
	int n,i,s,a[10],c=0;
	printf("Enter the total no. of num : ");
	scanf("%d", &n);
	printf("Enter the numbers : ");
	for(i=0; i<=n-1; i++)
	{
		scanf("%d", &a[i]);
	}
	 printf("Enter the search element : ");
	 scanf("%d", &s);
	 for(i=1; i<=n; i++)
	 {
	 	if(a[i]==s)
	 	{
	 		c=c+i;
		}
	 }
	 if(c==0)
	 {
	 	printf("Element is not found");
	  } 
	 else
	 {
	 	printf("Element is found at index %d", c);
	 }
}

